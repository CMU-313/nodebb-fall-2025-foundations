{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "command",
  "commandRunner": {
    "command": "npm run test:prod"
  },
  "coverageAnalysis": "off",
  "buildCommand": "npx cross-env NODE_ENV=production ./nodebb build",
  "symlinkNodeModules": true,
  "mutate": ["src/**/*.js", "!src/cli/**/*.js"],
  "ignorePatterns": ["build/**", "node_modules/**", "test/databasemock.js"]
}